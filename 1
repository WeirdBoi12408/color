import sys
for i in range(0, 43):
    for j in range(0, 6):
        code = str(i * 6 + j - 2)
        sys.stdout.write("\u001b[38;5;" + code + "m " + code.ljust(4))
    print("\u001b[0m")

# https://www.lihaoyi.com/post/BuildyourownCommandLinewithANSIescapecodes.html#rich-text
# Read about ANSI escape codes
